[run]
source = .
omit =
    */migrations/*
    */tests/*
    */__pycache__/*
    */venv/*
    */env/*
    manage.py
    */apps.py
    */admin.py
    */wsgi.py
    */asgi.py

[report]
precision = 2
show_missing = True
skip_covered = False
fail_under = 70

exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

